{
    "name": "iyogesharma/datatable-laravel",
    "description": "package to help in handling both server and client side datatable.js operations",
    "keywords": [
        "iYogesharrma",
        "datatable",
        "laravel"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Yogesh Sharma",
            "email": "iyogesharma@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "php": ">=7.0.0",
        "laravel/framework": "5.*|~6.0|~7.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.4",
        "orchestra/testbench": "~3.5.0|~3.6.0|~3.7.0|~3.8.0|~3.9.0"
    },
    "autoload": {
        "psr-4": {
            "YS\\Datatable\\": "src/"
        },
        "files": [
            "src/Helpers/helper.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "YS\\Datatable\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "extra": {
        "laravel": {
            "providers": [
                "YS\\Datatable\\DatatableServiceProvider"
            ],
            "aliases": {
                "Datatable":"YS\\Datatable\\Facades\\Datatable",
                "Table":"YS\\Datatable\\Facades\\Table"
            }
        },
        "branch-alias": {
            "dev-master": "2.x-dev"
        }
    }
}
